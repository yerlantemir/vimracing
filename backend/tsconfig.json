{
    "compilerOptions":{
        "rootDir": "src",
        "outDir": "dist",
        "resolveJsonModule": true,
        "lib": ["es6"],
        "module": "commonjs",
        "noImplicitAny": true,
        "strict": true,
        "esModuleInterop": true,
        "downlevelIteration": true
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}